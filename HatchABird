UpgradeDesc.TextSize = 10
UpgradeDesc.TextWrapped = true
UpgradeDesc.TextYAlignment = Enum.TextYAlignment.Top
UpgradeDesc.Parent = UpgradeScroll

-- Canvas Sizing
local function autoSize(layout, scroll)
    layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        scroll.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 10)
    end)
end

autoSize(EggsListLayout, EggsScroll)
autoSize(FoodsListLayout, FoodsScroll)
autoSize(UpgradeListLayout, UpgradeScroll)

-- Initialize tabs to show Eggs tab first
switchToTab("Eggs")
