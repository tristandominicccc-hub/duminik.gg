-- AUTO UPGRADE TAB CONTENT (NOW A TOGGLE)
local autoUpgradeLoop = false
local upgradeDelay = 0.5  -- Adjust this value as needed

local UpgradeToggleFrame = Instance.new("Frame")
UpgradeToggleFrame.Size = UDim2.new(1, -10, 0, 40)
UpgradeToggleFrame.Position = UDim2.new(0, 5, 0.5, -20)
UpgradeToggleFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
UpgradeToggleFrame.BorderSizePixel = 0
UpgradeToggleFrame.Parent = UpgradeScroll

local UpgradeToggleCorner = Instance.new("UICorner")
UpgradeToggleCorner.CornerRadius = UDim.new(0, 4)
UpgradeToggleCorner.Parent = UpgradeToggleFrame

local UpgradeLabel = Instance.new("TextLabel")
UpgradeLabel.Size = UDim2.new(0.65, -8, 0.6, 0)
UpgradeLabel.Position = UDim2.new(0, 6, 0, 2)
UpgradeLabel.BackgroundTransparency = 1
UpgradeLabel.Text = "Auto Plot Upgrade"
UpgradeLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
UpgradeLabel.TextXAlignment = Enum.TextXAlignment.Left
UpgradeLabel.Font = Enum.Font.GothamBold
UpgradeLabel.TextSize = 11
UpgradeLabel.Parent = UpgradeToggleFrame

local UpgradeToggleButton = Instance.new("TextButton")
UpgradeToggleButton.Size = UDim2.new(0, 50, 0, 22)
UpgradeToggleButton.Position = UDim2.new(1, -55, 0.5, -11)
UpgradeToggleButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
UpgradeToggleButton.BorderSizePixel = 0
UpgradeToggleButton.Text = "OFF"
UpgradeToggleButton.TextColor3 = Color3.fromRGB(255, 100, 100)
UpgradeToggleButton.Font = Enum.Font.GothamBold
UpgradeToggleButton.TextSize = 9
UpgradeToggleButton.Parent = UpgradeToggleFrame

local UpgradeButtonCorner = Instance.new("UICorner")
UpgradeButtonCorner.CornerRadius = UDim.new(0, 4)
UpgradeButtonCorner.Parent = UpgradeToggleButton

UpgradeToggleButton.MouseButton1Click:Connect(function()
    autoUpgradeLoop = not autoUpgradeLoop
    
    if autoUpgradeLoop then
        TweenService:Create(UpgradeToggleButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(60, 180, 80)}):Play()
        UpgradeToggleButton.Text = "ON"
        UpgradeToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        
        -- Start the auto upgrade loop
        task.spawn(function()
            while autoUpgradeLoop do
                pcall(function()
                    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Upgrade"):FireServer()
                end)
                task.wait(upgradeDelay)
            end
        end)
    else
        TweenService:Create(UpgradeToggleButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(80, 80, 80)}):Play()
        UpgradeToggleButton.Text = "OFF"
        UpgradeToggleButton.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
end)

-- Add a description label
